{
  "name": "minibox",
  "version": "0.0.4",
  "description": "Easy pop-ups and modals. Lightweight, and easier to use than 'that other one'.",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack/webpack.config.prod.js  --colors",
    "start": "webpack-dev-server --host 0.0.0.0 --open --config webpack/webpack.config.dev.js",
    "pack": "NODE_ENV=production webpack --config webpack.config.js  --colors"
  },
  "main": "src/scripts/index.js",
  "unpkg": "dist/minibox.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylewetton/minibox"
  },
  "keywords": [
    "modal",
    "popup",
    "lightbox"
  ],
  "author": "Kyle Wetton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kylewetton/minibox"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "autoprefixer": "^9.7.6",
    "core-js": "^3.3.2",
    "postcss-loader": "^3.0.0"
  }
}
